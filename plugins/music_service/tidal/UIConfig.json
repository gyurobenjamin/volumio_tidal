{
	"page": {
		"label": "TRANSLATE.PLUGIN_CONFIGURATION"
	},
	"sections": [{
		"id": "section_account",
		"element": "section",
		"label": "Tidal account",
		"icon": "fa-plug",
		"onSave": {
			"type": "controller",
			"endpoint": "music_service/tidal",
			"method": "saveTidalAccount"
		},
		"saveButton": {
			"label": "Save",
			"data": [
				"username",
				"password",
				"bitrate"
			]
		},
		"content": [{
				"id": "username",
				"type": "text",
				"element": "input",
				"doc": "This is the username of your Tidal account",
				"label": "Username",
				"value": ""
			},
			{
				"id": "password",
				"type": "password",
				"element": "input",
				"doc": "This is the password of your Tidal account",
				"label": "Password",
				"value": ""
			},
			{
				"id": "bitrate",
				"element": "switch",
				"doc": "High bitrate",
				"label": "Set for high bitrate",
				"value": true
			}
		]
	}]
}
